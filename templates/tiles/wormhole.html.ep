%
% my $wh = $tile->wormhole;
%
<use href="#<%= $tile->ring_id %>"/>
<circle cx="0" cy="0" r="15" fill="none" stroke="darkgrey" stroke-width="2"/>
<% if ($wh eq "alpha") { %>
<text class="wh" fill="goldenrod" text-anchor="middle" y="3">α</text>
<% } elsif ($wh eq "beta") { %>\
<text class="wh" fill="blue" text-anchor="middle" y="4">β</text>
<% } %>\
